//PATTERN PRINTING


#include <stdio.h>

int main() {
    int row = 4;

    for(int i = 1; i <= row; i++) {
        // Print spaces
        for(int j = 1; j <= row - i; j++) {
            printf(" ");
        }

        // Print stars
        for(int k = 1; k <= i; k++) {
            printf("* ");
        }

        printf("\n");
    }

    return 0;
}
